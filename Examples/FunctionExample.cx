D CalcArea(D x, D y).Do(
	Comment(Calculate the Area);
	D area<<x*y;
	Exit(area);
);

D x<<.0, y<<.0;

If(x!=0.2).Then(x<<5.9;);

On.Console("\nGive me the x: ");
x<<In.Console().toDecimal();

On.Console("\nGive me the y: ");
y<<In.Console().toDecimal();

D area <<F.CalcArea(x,y);
On.Console("\nThe Area: ");
On.Console(area.toString());
